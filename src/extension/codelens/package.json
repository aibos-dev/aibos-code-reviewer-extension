{
  "name": "codelens",
  "displayName": "CodeLens Review",
  "description": "Enhances code reviews with AI-powered inline annotations and insights.",
  "version": "0.0.1",
  "publisher": "codelens",
  "icon": "icon.png",
  "engines": { "vscode": "^1.97.0" },
  "categories": ["Other"],
  "activationEvents": [
      "onStartupFinished",
      "onCommand:codelens.startChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "commands": [
          {
              "command": "codelens.startChat",
              "title": "Start Code Review Chat"
          }
      ]
  },
  "scripts": {
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "test": "vscode-test"
  },
  "devDependencies": {
      "typescript": "^5.7.3",
      "@types/vscode": "^1.97.0",
      "@vscode/test-electron": "^2.4.1"
  },
  "dependencies": {
      "axios": "^1.7.9"
  }
}
